{
  "name": "rag-starter-kit",
  "version": "1.0.0",
  "description": "Simple RAG application for chatting with multimodal files",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "client": "npm run dev --prefix client",
    "client:build": "npm run build --prefix client",
    "client:start": "npm run preview --prefix client",
    "dev:all": "npm run dev --prefix client & npm run dev"
  },
  "keywords": [
    "rag",
    "llm",
    "vector-search",
    "typescript",
    "node"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ffmpeg-static": "^5.2.0",
    "mammoth": "^1.6.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^5.12.2",
    "pdf-parse": "^1.1.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/ffmpeg-static": "^3.0.3",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.0",
    "@types/pdf-parse": "^1.1.4",
    "@types/uuid": "^9.0.7",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.2"
  }
}
